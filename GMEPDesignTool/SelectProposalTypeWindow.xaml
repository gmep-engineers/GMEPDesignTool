<Window x:Class="GMEPDesignTool.SelectProposalTypeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GMEPDesignTool"
        mc:Ignorable="d"
        Title="Select Proposal Type" Height="250" Width="400">
    <Grid>
        <ComboBox HorizontalAlignment="Left" Margin="96,60,0,0" VerticalAlignment="Top" Width="120" SelectedValue="{Binding TypeId}" SelectedValuePath="Tag">
            <ComboBoxItem Tag="1">Commercial</ComboBoxItem>
            <ComboBoxItem Tag="1">Residential</ComboBoxItem>
            <ComboBoxItem Tag="1">T24</ComboBoxItem>
            <ComboBoxItem Tag="1">Site Lighting Tarrar</ComboBoxItem>
            <ComboBoxItem Tag="1">2019</ComboBoxItem>
        </ComboBox>

        <Button Click="SelectButton_Click" Content="Select" HorizontalAlignment="Left" Margin="136,0,0,0" VerticalAlignment="Center"/>
    </Grid>
</Window>
